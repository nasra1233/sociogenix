{
  "hosting": {
    "public": "build",
    "ignore": ["frontend_firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/oauth-callback",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
